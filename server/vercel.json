{
  "installCommand":"npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "/server/server.js",
      "use": "@vercel/node"
    },
    {
      "src":"src/**/*",
      "use":"@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server/server.js"
    }
  ]
}
